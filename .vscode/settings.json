      },
      "NOTE(gxf)": {
        "icon": "tag",
        "gutterIcon": true,

        "foreground": "#33db24",
        "rulerColour": "#33db24",
        "iconColour": "#33db24",
        "type": "text-and-comment",
      },
      "glider": {
          "icon": "tag",
          "foreground": "#4400ff",
          "rulerColour": "#4400ff",
          "iconColour": "#4400ff",
          "type": "text-and-comment",
        },
  
    },
