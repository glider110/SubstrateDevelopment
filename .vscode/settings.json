      "?(gxf)": {
        "icon": "question",
        "gutterIcon": true,
        "foreground": "red",
        "iconColour": "red",
        "type": "text-and-comment"
      },
      "!(gxf)": {
        "foreground": "yellow",
        "icon": "issue-opened",
        "gutterIcon": true,
        "iconColour": "yellow",
        "type": "text-and-comment"
      },
      "method": {
        "foreground": "#d3a0f0",
