{
    "doxdocgen.generic.paramTemplate": "@param[in] {param} ",

    // 在注释中搜索以 * // , # , ; , 开头的标签！ -- 或 / * 
    "todo-tree.regex.regex": "((\\*|//|#|<!--|;|/\\*|^)\\s*($TAGS):|^\\s*- \\[ \\])",
    
    "todo-tree.highlights.defaultHighlight": {
      "icon": "alert",
      "type": "text",
      "foreground": "red",
      "iconColour": "green"
    },
    "todo-tree.general.tags": [
      "glider ?",     //未理解
      "glider !",      //
      "glider note",  //注释                                
      "glider todo",  //待完成
    //   "param",
      "method",
    //   "c++11",
      "glider url",      //未定义
    ],

      //glider ?:   //未理解
       //glider !:   //重点
      //glider note: //注释
      //glider todo: //待完成
      //glider url:
      //method:

    "todo-tree.highlights.customHighlight": {
      "glider ?": {
        "icon": "question",
        "gutterIcon": true,
        "foreground": "red",
        "iconColour": "red",
        "type": "text-and-comment"
      },
      "glider !": {
        "foreground": "yellow",
        "icon": "issue-opened",
        "gutterIcon": true,
        "iconColour": "yellow",
        "type": "text-and-comment"
      },
      "method": {
        "foreground": "#d3a0f0",
        "icon": "question",
        "gutterIcon": true,
        "iconColour": "green",
        "type": "text-and-comment",
        "hideFromTree": true
      },
    //   "c++11": {
    //     "icon": "alert",
    //     "foreground": "violet",
    //     "rulerColour": "violet",
    //     "iconColour": "violet",
    //     "type": "text-and-comment",
    //   },
      "glider note": {
        "icon": "note",
        "gutterIcon": true,
        "foreground": "orange",
        "rulerColour": "orange",
        "iconColour": "orange",
        "type": "text-and-comment",
      },
      "param": {
        "icon": "issue-closed",
        "gutterIcon": true,
        "foreground": "orange",
        "rulerColour": "orange",
        "iconColour": "orange",
        "type": "text-and-comment",
      },
      "glider url": {
        "icon": "tools",
        "gutterIcon": true,
        "foreground": "#2e00fc",
        "rulerColour": "#2e00fc",
        "iconColour": "#2e00fc",
        "rulerLane": "full",
        "type": "line",
      },
      "glider todo": {
        "icon": "tag",
        "gutterIcon": true,

        "foreground": "#33db24",
        "rulerColour": "#33db24",
        "iconColour": "#33db24",
        "type": "text-and-comment",
      },
    //   "glider": {
    //       "icon": "tag",
    //       "foreground": "#4400ff",
    //       "rulerColour": "#4400ff",
    //       "iconColour": "#4400ff",
    //       "type": "text-and-comment",
    //     },
  
    },
    "python.autoComplete.extraPaths": [
      "/opt/ros/kinetic/lib/python2.7/dist-packages"
    ],
    "todo-tree.tree.showBadges": true,
    "todo-tree.general.revealBehaviour": "highlight todo",
    "todo-tree.tree.showCountsInTree": true,
