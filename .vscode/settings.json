        "foreground":  "#d3a0f0",
        "rulerColour": "orange",
        "iconColour": "orange",
        "type": "text-and-comment",
      },
      "glider url": {
        "icon": "tools",
        "gutterIcon": true,
        "foreground": "#2e00fc",
        "rulerColour": "#2e00fc",
        "iconColour": "#2e00fc",
